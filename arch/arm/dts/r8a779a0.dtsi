			clocks = <&cpg CPG_MOD 211>;
			power-domains = <&sysc R8A779A0_PD_ALWAYS_ON>;
			resets = <&cpg 211>;
			phy-mode = "rgmii";
			//iommus = <&ipmmu_ds1 0>;
			#address-cells = <1>;
			#size-cells = <0>;
			status = "disabled";
		};

